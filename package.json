{
  "name": "swan-z-style-app",
  "version": "1.0.0",
  "description": "An AI-driven mobile application for personalized fashion recommendations and custom designs",
  "main": "src/backend/server.ts",
  "scripts": {
    "start": "node dist/backend/server.js",
    "dev": "nodemon src/backend/server.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "mobile": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.0",
    "@react-navigation/stack": "^6.0.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.6",
    "mongoose": "^5.12.14",
    "pg": "^8.6.0",
    "react": "^17.0.2",
    "react-native": "^0.64.2",
    "react-redux": "^7.2.4",
    "redis": "^3.1.2",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "tensorflow": "^3.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.11",
    "@types/react-native": "^0.64.10",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/swan-z-style-app.git"
